<%- include('../partials/adminHeader.ejs') %>








<div>
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content borderradius-remove">
            <div class="modal-header d-flex justify-content-center">
                <h5 class="modal-title" id="staticBackdropLabel">Add New Banner</h5>
                <!-- <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button> -->
            </div>
            <div class="modal-body">
                <form action="/admin/addBanner" method="post" enctype="multipart/form-data" id="form" name="form">
                    <div class="form-outline">
                        <label for="resItemName" class="form-label">image</label>
                        <input type="file" id="Image" name="image" class="form-control shadow-remove borderradius-remove"
                            placeholder="Food Category"  >
                    </div>
                    <div class="form-outline">
                      <label for="resItemName" class="form-label">Name</label>
                      <input type="text"  name="Name" class="form-control shadow-remove borderradius-remove"
                          placeholder="description" id="resItemName" aria-describedby="emailHelp">
                  </div>
                    <br>
                   
    
            <div class="modal-footer d-flex justify-content-center">
                <button type="submit" 
                    class="btn btn-info text-white borderradius-remove shadow-remove" data-bs-dismiss="modal">
                    ADD
                </button>
              </form>
















