<%- include('../partials/adminHeader.ejs') %>












<div class="container-fluid py-4">
    <div class="row">
      <div class="col-12">
        <div class="card mb-4">
          <div class="card-header pb-0">
            <h6>Categorys</h6>
           
              <a href="/admin/addCoupon" class="btn btn-outline-success btn-sm" tabindex="-1" role="button" aria-disabled="true">add Coupen</a>
          </div>
            <form action="" method="post">
              <th><input type="text" class="serch-input" name="search" style=" outline: none; padding: 2px; min-width: 200px;" placeholder="Search By First Name"></th>
              <th><input type="submit" class="btn btn-outline-success btn-sm" value="Search"></th>
          </form>
          </div>
          <div class="card-body px-0 pt-0 pb-2">
            <div class="table-responsive p-0">
              <table class="table align-items-center mb-0">
                <thead>
                  <tr>
                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Coupon</th>
                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Offer (in %)</th>
                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Max Amount (in Rs.)</th>
                    <!-- <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Employed</th> -->
                    <th class="text-secondary opacity-7"></th>
                  </tr>
                </thead>
                <% allData.forEach(data =>{  %>
                <tbody>
                  <tr>
                    <td>
                      <div class="d-flex px-2 py-1">
                        <div>
                          <!-- <img src="/img/team-2.jpg" class="avatar avatar-sm me-3" alt="user1"> -->
                        </div>
                        <div class="d-flex flex-column justify-content-center">
                          <h6 class="mb-0 text-sm"><%= data.coupon_code %></h6>
                          <!-- <p class="text-xs text-secondary mb-0">john@creative-tim.com</p> -->
                        </div>
                      </div>
                    </td>
                    <td>
                      <p class="text-xs font-weight-bold mb-0"><%= data.offer  %></p>
                      <!-- <p class="text-xs text-secondary mb-0">Organization</p> -->
                    </td>
                    <td>
                        <p class="text-xs font-weight-bold mb-0"><%= data.max_amount   %></p>
                        <!-- <p class="text-xs text-secondary mb-0">Organization</p> -->
                      </td>
                   
                    <td class="align-middle text-center">
                      <!-- <span class="text-secondary text-xs font-weight-bold">23/04/18</span> -->
                    </td>
                    <!-- <td class="align-middle">
                      <a href="/admin/editCategory/<%= data._id %>" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                        Edit
                      </a>
                    </td> -->
                    <td class="align-middle">
                      <a   href="/admin/deleteCoupon/<%= data._id %>"" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                        remove
                      </a>
                    </td>
                  </tr>
                
                </tbody>
                <%})%>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>









<%- include('../partials/footer.ejs') %>